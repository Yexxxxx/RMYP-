<view class='content'>
  <view class='nav'>
    <view class='top'>
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class='nickname'>
      <open-data type="userNickName"></open-data> 
    </view>
  </view>
    <view class='item'>
            <text class='left'>性别：</text>
          <text  class='right'>{{gender}}</text>
    </view>
    <view class='item'>
          <text class='left'>身高：</text>
          <text class='right'>{{height}}</text>
    </view>
    <view class='item'>
          <text class='left'>体重：</text>
          <text class='right'>{{weight}}</text>
    </view>
    <view class='item'>
        <text class='left'> 年龄：</text>
        <text class='right'>{{age}}</text>
    </view>
    <view class='item'>
          <text class='left'>手机：</text>
          <text class='right'>{{phone}}</text>
    </view>
    <view class='item' bindtap='exit'>
          <text class='left'>退出：</text>
          <text class='right'>>>></text>
    </view>
  
</view>

