/* pages/analysisPart/ana.wxss */
ec-canvas {
  width: 110%;
  height: 100%;
}
.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

.section{
  text-align: center;
  position: relative;
  top:20px;
  width: 100px;
  height: 45px;
}
.weight{
  border: #5E7CE2 solid 1px;
  background-color: hsl(191, 24%, 78%);
  border-radius: 5px;
}
.height{
  position: relative;
  top:-5px;
  border: #5E7CE2 solid 1px;
  background-color: hsl(191, 24%, 78%);
  border-radius: 5px;

}
